{
  "hosting": {
    "public": "dist/notus-angular",
    "rewrites": [
      {
        "source": "/api/**",
        "destination": "https://logistwork.onrender.com"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],

    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
